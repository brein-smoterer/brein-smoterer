@page "/characters"
@using Genshin_Site.Data;

<h3>Character</h3>
@foreach (var character in roster)
{
    <img src="@character.Imege" asp-append-version="true" width="180px" />
    <br>
    <button @onclick = "() => Show(character)">@character.CharacterName:</button>
    <div hidden="@(!character.Show)">
        <div>@character.Element</div>
        <div>@character.Weapon</div>
    </div>
    <br>
}

